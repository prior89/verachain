{
  "name": "verachain-mobile-web",
  "version": 2,
  "builds": [
    {
      "src": "mobile/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "mobile/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/mobile/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {},
  "redirects": [
    {
      "source": "/",
      "destination": "/mobile/dist/index.html",
      "permanent": false
    }
  ]
}